		b: 0.4333333
		a: 1
	}
	white: {
		r: 1
		g: 1
		b: 1
		a: 1
	}
}
com.badlogic.gdx.scenes.scene2d.ui.Skin$TintedDrawable: {
	pale-blue: {
		name: white
		color: {
			r: 0.48342222
			g: 0.76367503
			b: 0.99333334
			a: 1
		}
	}
	black: {
		name: white
		color: {
			r: 0
			g: 0
			b: 0
			a: 1
		}
	}
